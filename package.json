{
  "name": "lms-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "db:prod:prisma:studio": "dotenv -e .env.prod prisma studio",
    "db:test:prisma:studio": "dotenv -e .env.test prisma studio",
    "db:prod:prisma:migrate": "dotenv -e .env.prod prisma migrate deploy",
    "db:prod:seed": "dotenv -e .env.prod ts-node src/seeder/seed.ts",
    "db:seed": "pnpm dlx prisma db seed",
    "db:generate": "pnpm dlx prisma generate",
    "db:studio": "pnpm dlx prisma studio",
    "docker:db:dev:rm": "docker compose rm dev-db -s -f -v",
    "docker:db:dev:up": "docker compose up dev-db -d",
    "docker:db:dev:restart": "docker compose rm dev-db -s -f -v && docker compose up dev-db -d && sleep 1 && prisma migrate dev && pnpm run db:seed",
    "docker:db:test:rm": "docker compose rm test-db -s -f -v",
    "docker:db:test:up": "docker compose up test-db -d",
    "docker:db:test:restart": "docker compose rm test-db -s -f -v && docker compose up test-db -d && sleep 1 && dotenv -e .env.test -- prisma migrate deploy",
    "migrate:deploy": "pnpm dlx prisma migrate deploy",
    "migrate:dev:create": "pnpm dlx prisma migrate dev --create-only",
    "migrate:dev": "pnpm dlx prisma migrate dev",
    "prisma-gen": "cat ./prisma/schemas/*.prisma > ./prisma/schema.prisma && prisma format",
    "prisma:sql": "npx prisma generate --sql",
    "prisma:sql:dev": "npx prisma generate --sql --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pnpm run migrate:deploy && node dist/main",
    "repl:dev": "nest start --watch '--entryFile' 'repl'",
    "repl": "nest start '--entryFile' 'repl'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretest": "dotenv -e .env.test prisma migrate reset -- --force && dotenv -e .env.test prisma migrate deploy && dotenv -e .env.test pnpm run db:seed",
    "pretest:github:actions": "dotenv -e .env.github prisma migrate reset -- --force",
    "test": "dotenv -e .env.test jest",
    "test:github:actions": "pnpm run pretest:github:actions && dotenv -e .env.github jest",
    "test:watch": "dotenv -e .env.test jest --watch",
    "test:cov": "pnpm run pretest && dotenv -e .env.test jest -- --runInBand --coverage",
    "test:debug": "dotenv -e .env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@casl/prisma": "^1.5.1",
    "@fastify/formbody": "^7.3.0",
    "@fastify/static": "^6.5.1",
    "@google-cloud/storage": "^7.11.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/platform-fastify": "^10.4.4",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.2.1",
    "@pdf-lib/fontkit": "^1.1.1",
    "@prisma/client": "^5.22.0",
    "@webundsoehne/nest-fastify-file-upload": "^2.2.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "cache-manager": "^5.7.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dinero.js": "^1.9.1",
    "dotenv-cli": "^7.4.2",
    "fastify": "^5.2.1",
    "fastify-multer": "^2.0.3",
    "financial": "^0.2.2",
    "json-2-csv": "^5.5.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mustache": "^4.2.0",
    "nestjs-prisma": "^0.23.0",
    "nestjs-redox": "^1.2.2",
    "nodemailer": "^7.0.4",
    "papaparse": "^5.5.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "ra-data-simple-prisma": "^5.2.1",
    "redoc": "^2.1.5",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "teeny-request": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/testing": "^10.4.1",
    "@types/cron": "^2.4.0",
    "@types/dinero.js": "^1.9.4",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "29.2.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.13.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "testSequencer": "../test/custom-sequencer.js",
    "maxWorkers": 1
  },
  "prisma": {
    "seed": "ts-node src/seeder/seed.ts"
  }
}
